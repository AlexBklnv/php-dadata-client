{
  "name": "alexbklnv/php-dadata-client",
  "prefer-stable": true,
  "description": "DaData API Client",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "alexbklnv",
    "dadata",
    "php",
    "address",
    "profile"
  ],
  "authors": [
    {
      "name": "Alexey Baklanov",
      "email": "alexbklnv@yandex.ru"
    }
  ],
  "require": {
    "php": "^7.4 | ^8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "PhpDaData\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
